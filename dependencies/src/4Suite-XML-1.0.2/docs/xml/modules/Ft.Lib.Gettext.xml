  <functions>
    <function public="no" name="FindCatalogs" id="FindCatalogs">
      <abstract># Locate a .mo file using the gettext strategy</abstract>
      <description/>
      <arguments>
        <arg name="domain"/>
        <arg default="None" name="localedir"/>
        <arg default="None" name="languages"/>
      </arguments>
    </function>
    <function public="yes" name="GetTranslation" id="GetTranslation">
      <abstract/>
      <description/>
      <arguments>
        <arg name="domain"/>
        <arg default="None" name="localedir"/>
        <arg default="None" name="languages"/>
        <arg default="None" name="class_"/>
        <arg default="False" name="fallback"/>
        <arg default="None" name="codeset"/>
        <arg default="None" name="bundle"/>
      </arguments>
